{
  "name": "@prantlf/gulp-jsonlint",
  "version": "0.0.0-development",
  "description": "A jsonlint plugin for Gulp",
  "author": "Ferdinand Prantl <prantlf@gmail.com> (http://prantl.tk)",
  "contributors": [
    "RogÃ©rio Vicente (https://github.com/rogeriopvl)",

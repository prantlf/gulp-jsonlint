    }
  ],
  "homepage": "https://github.com/prantlf/gulp-jsonlint",
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/gulp-jsonlint.git"
  },
  "bugs": "https://github.com/prantlf/gulp-jsonlint/issues",
  "main": "./index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc --silent mocha",
    "posttest": "nyc --silent --no-clean npm run examples || nyc report",
    "lint": "eslint **/*.js",
    "lint:fix": "prettier --write index.js lib/**/*.js test/**/*.js",
    "examples": "gulp valid && gulp one || gulp two || gulp three || gulp all"
  },
  "keywords": [
    "gulpplugin",
    "jsonlint",
    "json",
    "lint"
  ],
  "dependencies": {
    "ansi-colors": "4.1.1",
    "fancy-log": "1.3.3",
    "@prantlf/jsonlint": "10.0.0",
    "map-stream": "0.0.7",
    "plugin-error": "1.0.1",
    "through2": "3.0.1"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "gulp": "4.0.2",
    "mocha": "6.2.2",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "should": "13.2.3",
    "vinyl": "2.2.0"
  }
